<%= form_for @project do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :location%>
    <%= f.text_field :location%>
    <%= f.label :area%>
    <%= f.text_field :area%>
    <%= f.label :style%>
    <%= f.text_field :style%>
    <%= f.label :start_date, 'Start Date'%>
    <%= f.text_field :start_date, :placeholder => 'YYYY-MM-DD'%>
    <%= f.label :estimate_time_finish, 'Estimate time of completion'%>
    <%= f.text_field :estimate_time_finish, :placeholder => 'YYYY-MM-DD'%>
    <%= f.label :cost%>
    <%= f.text_field :cost%>
    <%= f.label :image%>
    <%= f.text_field :image%>
    <%= f.submit%>
<% end %>